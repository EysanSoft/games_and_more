      <Label fx:id="labelEstadoCaja" layoutX="20.0" layoutY="25.0" text="La caja tiene en este momento: $0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="labelEstadoProductos" layoutX="20.0" layoutY="60.0" text="El numero de productos vendidos es de: 0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
