<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: purple;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.visual.mySQL.Controller.ViewArqueoDeCajaController">
   <children>
      <Label fx:id="labelEstadoCaja" layoutX="20.0" layoutY="40.0" text="La caja tiene en este momento: $0" textFill="YELLOW">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="botonRetirar" layoutX="240.0" layoutY="130.0" mnemonicParsing="false" onMouseClicked="#botonRetirarOnMouseClicked" text="Retirar" />
      <Button fx:id="botonSalir" layoutX="340.0" layoutY="130.0" mnemonicParsing="false" onMouseClicked="#botonSalirOnMouseClicked" text="Salir" />
      <TextField fx:id="textFieldRetirar" layoutX="20.0" layoutY="130.0" prefHeight="26.0" prefWidth="219.0" promptText="Monto a retirar" />
   </children>
</AnchorPane>
